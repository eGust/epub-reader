{
  "name": "epub-reader",
  "productName": "epub-reader",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "sb": "start-storybook --ci -p 4321",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "keywords": [],
  "author": {
    "name": "eGust",
    "email": "egustc@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "epub_reader"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./app/renderer/assets/index.html",
                  "js": "./app/renderer/index.ts",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@electron-forge/cli": "6.0.0-beta.47",
    "@electron-forge/maker-deb": "6.0.0-beta.47",
    "@electron-forge/maker-rpm": "6.0.0-beta.47",
    "@electron-forge/maker-squirrel": "6.0.0-beta.47",
    "@electron-forge/maker-zip": "6.0.0-beta.47",
    "@electron-forge/plugin-webpack": "6.0.0-beta.47",
    "@hot-loader/react-dom": "^16.11.0",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-info": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-viewport": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/preset-typescript": "^1.2.0",
    "@storybook/react": "^5.2.8",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/history": "^4.7.3",
    "@types/jszip": "^3.1.6",
    "@types/lodash": "^4.14.149",
    "@types/nedb": "^1.8.9",
    "@types/node": "^12.12",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-hot-loader": "^4.1.1",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/webpack": "^4.41.1",
    "@types/webpack-env": "^1.14.1",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.0.0",
    "electron": "7.1.7",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "node-loader": "^0.6.0",
    "react-docgen-typescript-loader": "^3.6.0",
    "redux-devtools-extension": "^2.13.8",
    "style-loader": "^1.1.2",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.0",
    "webpack": "^4.41.5",
    "worker-loader": "^2.0.0"
  },
  "dependencies": {
    "@uifabric/react-hooks": "^7.0.1",
    "connected-react-router": "^6.6.1",
    "custom-electron-titlebar": "^3.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "history": "^4.10.1",
    "jszip": "^3.2.2",
    "lodash": "^4.17.15",
    "nedb": "^1.8.0",
    "office-ui-fabric-core": "^11.0.0",
    "office-ui-fabric-react": "^7.77.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.18",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "system-font-families": "^0.4.1"
  },
  "resolutions": {
    "marked": "^0.8.0"
  }
}
