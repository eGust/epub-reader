{
  "env": {
    "development": {
      "application/javascript": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "electron": "1.4.13"
              }
            }
          ],
          "es2015",
          "react"
        ],
        "plugins": [
          "transform-async-to-generator",
          "transform-object-rest-spread",
          "transform-class-properties"
        ],
        "sourceMaps": "inline"
      }
    },
    "production": {
      "application/javascript": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "electron": "1.4.13"
              }
            }
          ],
          "es2015",
          "react"
        ],
        "plugins": [
          "transform-async-to-generator",
          "transform-object-rest-spread",
          "transform-class-properties"
        ],
        "sourceMaps": "none"
      }
    }
  },
  "application/javascript": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  }
}